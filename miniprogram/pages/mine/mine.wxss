.header{
  width: 98%;
  border: 1px solid #cfcfcf;
  display: flex;
  align-items: center;
  border-radius: 5px;
  margin-top: 20px;
  padding: 15px 0 15px 0;
  position: relative;
}
.avatar{
  width: 64px;
  height: 64px;
  position: relative;
  margin-left: 10px;
}
.avatar image{
  position: relative;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.icon{
  width: 18px;
  height: 18px;
  border: 1px solid #000;
  margin-left: 15px;
}
.name{
  margin-left: 15px;
  font-size: 18px;
}
.arrow_left{
  width: 18px;
  height: 18px;
  position: absolute;
  right: 0;
}
.body{
  display: block;
  position: relative;
  width: 70%;
  margin: 0 auto;
  margin-top: 40px;
  border-radius: 8px;
  overflow: hidden;
}
.body .box{
  position: relative;
  display: inline-block;
  width: 48%;
  text-align: center;
  background-color: lightblue;
  padding: 25px 0 35px 0;
}
.setting-wrapper, .helper-wrapper{
  margin-right: 4%;
}
.history-wrapper, .setting-wrapper{
  margin-bottom: 4%;
}
.image-icon{
  width: 32px;
  height: 32px;
  position: relative;
  margin: 0 auto;
}
.image-icon .icons{
  width: 100%;
  height: 100%;
}
.arrow_left_container {
  flex: 1;
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
}